class ZMQServer:
    def __init__(self, server_endpoint: str) -> None: ...

    def add_topic(self, topic: str, max_remaining_time: float) -> None: ...

    def put_data(self, topic: str, data: bytes) -> None: ...

    def get_latest_data(self, topic: str) -> bytes: ...

    def get_last_k_data(self, topic: str, k: int) -> list[bytes]: ...

    def get_all_data(self, topic: str) -> list[bytes]: ...

    def get_all_topic_names(self) -> list[str]: ...

class ZMQClient:
    def __init__(self, server_endpoint: str) -> None: ...

    def request_latest(self, topic: str) -> bytes: ...

    def request_all(self, topic: str) -> list[bytes]: ...

    def request_last_k(self, topic: str, k: int) -> list[bytes]: ...

    def request_with_data(self, topic: str, data: bytes) -> bytes: ...

    